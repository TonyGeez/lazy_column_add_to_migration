{
    "name": "tonygeez/lazy-column-add-to-migration",
    "description": "Laravel package to add columns to migrations via Artisan commands.",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "laravel/tinker": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "TonyGeez\\LazyColumnAddToMigration\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TonyGeez\\LazyColumnAddToMigration\\LazyColumnServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Tony Geez",
            "email": "hixostorage@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true
}

